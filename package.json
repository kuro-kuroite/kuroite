  "license": "MIT",
  "author": "kuro-kuroite <26164343+kuro-kuroite@users.noreply.github.com>",
  "scripts": {
    "build": "scripts build",
    "build-storybook": "build-storybook",
    "ci:check": "run-p ci:check:type ci:check:format ci:check:commit ci:check:config ci:check:md --continue-on-error",
    "ci:check:commit": "scripts checkCommit",
    "ci:check:config": "scripts checkConfig",
    "ci:check:css": "scripts checkCss",
    "ci:check:format": "run-p ci:check:css ci:check:ts --continue-on-error",
    "ci:check:md": "scripts checkMd",
    "ci:check:ts": "scripts checkTs",
    "ci:check:type": "scripts checkType",
    "ci:create:issue": "scripts createIssue",
    "ci:test": "run-p ci:test:* --continue-on-error",
    "ci:test:e2e": "npm run test:e2e",
    "ci:test:seo": "npm run test:seo",
    "ci:test:styleguide": "npm run test:styleguide",
    "ci:test:unit": "npm run test:unit",
    "ci:upload:coverage": "scripts uploadCoverage",
    "clean": "scripts clean",
    "copyStaticQueries": "scripts copyStaticQueries",
    "deploy": "scripts deploy",
    "develop": "scripts develop",
    "e2e": "scripts e2e",
    "fix": "scripts fix",
    "fix:npm": "npx yarn-audit-fix ; echo '`npm run test:all` to check no regression'",
    "postinstall": "npx patch-package && npx typesync",
    "lint": "npm run ci:check",
    "ncu": "ncu --target minor",
    "prepare": "npm run scripts:build",
    "release": "scripts release",
    "scripts:build": "cd scripts/ && npx pastel build && npm link .",
    "scripts:dev": "cd scripts/ && npx pastel dev",
    "serve": "scripts serve",
    "start": "npm run develop",
    "start-server-and-test": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test",
    "start-storybook": "start-storybook -p 6006",
    "storycap": "storycap --serverTimeout 120000 http://localhost:6006",
    "styleguide": "scripts styleguide",
    "tcm": "tcm -p src/**/*.module.css",
    "test": "npm run test:unit",
    "test:all": "run-s test:unit test:styleguide test:e2e test:seo --continue-on-error",
    "test:e2e": "scripts testE2e",
    "test:npm": "yarn outdated ; yarn audit",
    "test:seo": "scripts testSeo",
    "test:styleguide": "scripts testStyleguide",
    "test:styleguide:a11y": "scripts testStyleguideA11y",
    "test:styleguide:a11y:script": "scripts testStyleguideA11yScript",
    "test:styleguide:visual": "scripts testStyleguideVisual",
    "test:styleguide:visual:script": "scripts testStyleguideVisualScript",
    "test:unit": "scripts testUnit",
    "update:e2e": "scripts updateE2e",
    "update:npm": "scripts updateNpm",
    "update:styleguide": "scripts updateStyleguide",
    "update:unit": "scripts updateUnit"
  },
