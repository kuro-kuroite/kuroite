{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch eslint",
      "program": "${workspaceFolder}/.eslintrc.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
